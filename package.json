{
  "name": "hapi-react-redux-example",
  "version": "1.0.0",
  "description": "Example app using github api",
  "scripts": {
    "babel": "babel src -d build -D -s",
    "babel:watch": "babel src -d build -w -D -s inline -q",
    "build": "yarn run clean && yarn run standard && yarn run babel && yarn run webpack -- -p",
    "clean": "rimraf './build'",
    "server:watch": "just-wait -p './build/webpack-assets.json' && nodemon --watch build --watch ./glue-config.json ./build/bin/index.js",
    "standard": "standard --fix --verbose | snazzy",
    "standard:watch": "onchange -i 'src/**/*.js' -- yarn run standard",
    "start": "if-env NODE_ENV=production && yarn run start:prod || yarn run start:dev",
    "start:dev": "yarn run clean && concurrently -p \"[{name}]\" -n \"BABEL,WEBPACK,STANDARDJS,HAPI\" -c \"black.bgWhite.bold,black.bgYellow.bold,black.bgGreen.bold,black.bgCyan.bold\" \"yarn run babel:watch\" \"yarn run webpack:watch\" \"yarn run standard:watch\" \"yarn run server:watch\"",
    "start:prod": "node ./build/bin/index.js",
    "webpack": "webpack",
    "webpack:watch": "webpack-dev-server"
  },
  "author": "Kelly Milligan",
  "license": "ISC",
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "async": "^1.5.0",
    "babel-polyfill": "^6.6.1",
    "boom": "^4.2.0",
    "classnames": "^2.2.5",
    "confidence": "^3.0.2",
    "dotenv": "^2.0.0",
    "glue": "^3.2.0",
    "good": "^7.0.2",
    "good-console": "^6.2.0",
    "good-squeeze": "^5.0.0",
    "hapi": "^16.0.2",
    "hapi-react-redux": "^1.4.1",
    "history": "^2.1.2",
    "hoek": "^3.0.4",
    "inert": "^4.0.2",
    "joi": "^8.0.3",
    "lodash": "^4.16.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^2.3.1",
    "react-redux": "^5.0.1",
    "react-redux-transition-manager": "^1.0.0",
    "react-router": "^3.0.0",
    "react-router-fetch": "^1.0.1",
    "react-router-redux": "^4.0.6",
    "react-router-scroll": "^0.3.2",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "superagent": "^3.3.0",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.6.4",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "eslint": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-standard": "latest",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "if-env": "^1.0.0",
    "jest": "^16.0.2",
    "json-loader": "^0.5.4",
    "just-wait": "^1.0.9",
    "node-sass": "^4.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "onchange": "^3.2.0",
    "postcss": "^5.2.6",
    "postcss-loader": "^1.2.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.0",
    "snazzy": "^5.0.0",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "webpack": "2.2.0-rc.1",
    "webpack-dev-server": "2.2.0-rc.0"
  }
}
